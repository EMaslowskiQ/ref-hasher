{
  "name": "ref-hasher",
  "version": "0.1.0",
  "description": "Universal hashing library with automatic engine selection",
  "homepage": "https://github.com/EMaslowskiQ/ref-hasher#readme",
  "bugs": {
    "url": "https://github.com/EMaslowskiQ/ref-hasher/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EMaslowskiQ/ref-hasher.git"
  },
  "license": "ISC",
  "author": "",
  "type": "module",
  "main": "./dist/hasher.js",
  "module": "./dist/hasher.js",
  "types": "./dist/hasher.d.ts",
  "exports": {
    ".": {
      "import": "./dist/hasher.js",
      "types": "./dist/hasher.d.ts"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "prepare": "npm run build",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run build",
    "typecheck": "tsc --noEmit",
    "test": "echo \"Error: no test specified\" && exit 1",
    "yalc:publish": "npm run build && yalc publish --push",
    "watch:publish": "tsc --watch --preserveWatchOutput & chokidar \"dist/**/*.js\" -c \"yalc publish --push\""
  },
  "dependencies": {
    "hash-wasm": "^4.12.0"
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "typescript": "^5.9.3"
  }
}
